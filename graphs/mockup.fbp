
'{default: state}' -> PREVIOUS next(NextState)
next OUT -> PREVIOUS next

'CSS' -> layout(css/ApplyStylesheet)

# UI elements
'#map' -> m(dom/EnsureComponent) -> MOUNT mapView(ParkingMap)
next -> mapProps(MakeMap) -> mapView
mapView EVENT -> next

'#details' -> d(AddToBody) -> MOUNT detailsView(material-ui/List)
next -> detailProps(MakeList) -> detailsView

'#input' -> i(AddToBody) -> MOUNT input(material-ui/Input)
next -> inputProps(MakeInput) -> input
input EVENT -> next

